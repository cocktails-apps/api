{
    "builds": [
        {
            "src": "coctails_api/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "coctails_api/app.py"
        }
    ],
    "env": {
        "APP_MODULE": "coctails_api.main:app"
    }
}
